# Supabase Configuration
# Copy this file to .env and fill in your actual values

# Project URL - Get this from your Supabase dashboard
EXPO_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co

# Supabase Anonymous Key (Public) - Safe to expose in client-side code
EXPO_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here

# Supabase Service Role Key (Secret - Server-side only)
# WARNING: Never expose this key in client-side code!
# Only use this for server-side operations like user deletion
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# Expo Project ID for Push Notifications
EXPO_PUBLIC_PROJECT_ID=--------------

# For React Native / Expo compatibility
NEXT_PUBLIC_SUPABASE_URL=https://your-project-id.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_anon_key_here
